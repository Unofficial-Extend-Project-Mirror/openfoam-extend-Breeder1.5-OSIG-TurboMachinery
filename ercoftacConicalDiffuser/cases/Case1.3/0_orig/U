FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (11.6 0 0);

boundaryField
{
    axis
    {
        type            symmetryPlane;
    }

    inlet           
    {
        type            fixedValue;
        value           uniform (11.6 0 0);

    }

    outlet          
    {
        type            zeroGradient;
    }

    wallProlongation
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    wallDiffuser
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    statSwirlWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    rotSwirlWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    back
    {
        type            wedge;
    }
    front
    {
        type            wedge;
    }

}

