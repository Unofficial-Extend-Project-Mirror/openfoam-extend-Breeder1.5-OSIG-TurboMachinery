FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    inlet           
    {
        type              profile1DfixedValue;
        fileName          "OFturboWGcase0.csv";
        fileFormat        "turboCSV";
        interpolateCoord  "R";
        fieldName         "K";
    }

    outlet          
    {
        type            zeroGradient;
    }

    wallDiffuser
    {
        type            zeroGradient;
    }

}

