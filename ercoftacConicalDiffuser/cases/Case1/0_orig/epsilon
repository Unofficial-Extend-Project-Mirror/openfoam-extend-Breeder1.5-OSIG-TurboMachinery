FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          epsilon;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 896.10908;

boundaryField
{
    inlet           
    {
        type            fixedValue;
        value           uniform 896.10908;
    }

    outlet          
    {
        type            zeroGradient;
    }

    wallProlongation
    {
        type            zeroGradient;
    }

    wallDiffuser
    {
        type            zeroGradient;
    }

    statSwirlWall
    {
        type            zeroGradient;
    }

    rotSwirlWall
    {
        type            zeroGradient;
    }

}


