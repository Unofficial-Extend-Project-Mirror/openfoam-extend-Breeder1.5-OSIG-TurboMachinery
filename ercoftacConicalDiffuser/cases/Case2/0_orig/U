FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 11.6);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 11.6);
    }
    outlet
    {
        type            zeroGradient;
    }
    wallOutletPipe
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wallDiffuser
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    statSwirlWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wallDump
    {
        type            fixedValue;   
        value           uniform (0 0 0);
     } 

    rotSwirlWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

}

