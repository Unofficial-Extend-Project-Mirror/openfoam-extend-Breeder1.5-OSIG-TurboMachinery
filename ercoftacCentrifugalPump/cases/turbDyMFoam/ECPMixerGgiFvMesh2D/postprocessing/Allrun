#!/bin/bash

time=0.295711;


###--------postprocessing----###

foamCalc mag U -case .. -time ${time}
foamCalc components U -case .. -time ${time}

###--------sampling---------###

sample -case .. -time ${time}
cp ../sets/${time}/test_magU_p_k_epsilon_Ux_Uy_Uz.xy .

paste test_magU_p_k_epsilon_Ux_Uy_Uz.xy teta > postProcessing_data_unsteady_${time}


time=0.296513;

foamCalc mag U -case .. -time ${time}
foamCalc components U -case .. -time ${time}

sample -case .. -time ${time}
cp ../sets/${time}/test_magU_p_k_epsilon_Ux_Uy_Uz.xy .
paste test_magU_p_k_epsilon_Ux_Uy_Uz.xy teta > postProcessing_data_unsteady_${time}


time=0.298118;

foamCalc mag U -case .. -time ${time}
foamCalc components U -case .. -time ${time}

sample -case .. -time ${time}
cp ../sets/${time}/test_magU_p_k_epsilon_Ux_Uy_Uz.xy .
paste test_magU_p_k_epsilon_Ux_Uy_Uz.xy teta > postProcessing_data_unsteady_${time}


time=0.298921;

foamCalc mag U -case .. -time ${time}
foamCalc components U -case .. -time ${time}

sample -case .. -time ${time}
cp ../sets/${time}/test_magU_p_k_epsilon_Ux_Uy_Uz.xy .
paste test_magU_p_k_epsilon_Ux_Uy_Uz.xy teta > postProcessing_data_unsteady_${time}


time=0.299322;

foamCalc mag U -case .. -time ${time}
foamCalc components U -case .. -time ${time}

sample -case .. -time ${time}
cp ../sets/${time}/test_magU_p_k_epsilon_Ux_Uy_Uz.xy .
paste test_magU_p_k_epsilon_Ux_Uy_Uz.xy teta > postProcessing_data_unsteady_${time}

###----------------gnuplot---------------###
gnuplot call_backward_linearUpwind.gplt


time=0.299723;

###--------postprocessing----###

foamCalc mag U -case .. -time ${time}
foamCalc components U -case .. -time ${time}

###--------sampling---------###

###--------part1----------###
cp ../system/sampleDict_part1 ../system/sampleDict
sample -case .. -time ${time}

